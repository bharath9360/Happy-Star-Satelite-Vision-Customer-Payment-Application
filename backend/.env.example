# ─── Copy this to .env and fill in real values ───────────────────────────────
# These same keys must be set as Environment Variables in Vercel Dashboard

# Supabase
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_KEY=eyJ...your-service-role-key...

# Auth
JWT_SECRET=your_strong_random_jwt_secret_here

# Razorpay (use live keys for production, test keys for testing)
RAZORPAY_KEY_ID=rzp_live_XXXXXXXXXXXXXX
RAZORPAY_KEY_SECRET=your_razorpay_secret

# CORS — set to your actual Netlify URL after deploy
FRONTEND_URL=https://your-app-name.netlify.app

# Not needed on Vercel (Vercel sets PORT automatically)
# PORT=5000